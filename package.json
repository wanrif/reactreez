{
  "name": "reactreez",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite",
    "start": "bunx --bun vite",
    "build": "tsc && bunx --bun vite build",
    "lint": "bunx biome lint --write ./src",
    "preview": "vite preview",
    "format": "bunx biome format --write --indent-style=space --indent-width=2 --jsx-quote-style=single ./src",
    "test": "vitest --changed --coverage.enabled --coverage.cleanOnRerun",
    "test:watch": "vitest --coverage.enabled --coverage.all --coverage.cleanOnRerun",
    "test:coverage": "vitest run --no-isolate --coverage.enabled --coverage.all --coverage.clean --coverage.thresholds.autoUpdate=true",
    "biome:explain": "bunx biome explain daemon-logs",
    "biome:clean": "bunx biome clean"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@reduxjs/toolkit": "^2.2.7",
    "axios": "^1.7.7",
    "fast-json-stable-stringify": "^2.1.0",
    "i18next": "^23.15.1",
    "lodash": "^4.17.21",
    "lz-string": "^1.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^14.1.3",
    "react-icons": "^5.3.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.2",
    "redux-persist": "^6.0.0",
    "zipson": "^0.2.12",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/bun": "^1.1.9",
    "@types/lodash": "^4.17.7",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@vitest/coverage-v8": "^1.6.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.11",
    "jsdom": "^25.0.0",
    "postcss": "^8.4.45",
    "rollup-obfuscator": "^4.1.1",
    "tailwindcss": "^3.4.11",
    "typescript": "^5.6.2",
    "vite": "^5.4.4",
    "vite-plugin-node-polyfills": "^0.22.0",
    "vitest": "^1.6.0"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@swc/core",
    "esbuild",
    "javascript-obfuscator"
  ]
}
